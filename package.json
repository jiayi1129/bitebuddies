{
  "name": "bitebuddies",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all init dev:init --parallel dev:server dev:client",
    "build": "tsc && vite build",
    "init": "convex init",
    "dev:server": "convex dev",
    "dev:client": "vite --open --clearScreen false",
    "dev:init": "convex dev --once"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.9",
    "convex": "^0.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "npm-run-all": "^4.1.5"
  }
}
